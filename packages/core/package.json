{
  "name": "@vue-admin/core",
  "version": "1.0.0",
  "description": "Core components of vue-admin, a frontend Framework for building admin applications on top of REST services, using ES6, Vue",
  "files": [
    "*.md",
    "lib",
    "esm",
    "src"
  ],
  "main": "lib/index",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "sideEffects": false,
  "authors": [
    "Nikolay Kostyurin <jilizart@gmail.com>"
  ],
  "repository": "JiLiZART/vue-admin",
  "homepage": "https://github.com/JiLiZART/vue-admin#readme",
  "bugs": "https://github.com/JiLiZART/vue-admin/issues",
  "license": "MIT",
  "scripts": {
    "build": "npm run build-cjs && npm run build-esm",
    "build-cjs": "rimraf ./lib && tsc",
    "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
    "watch": "tsc --outDir esm --module es2015 --watch"
  }
}
